{
  "manifest_version": 3,
  "name": "Example",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:*/*", "http://127.0.0.1:*/*"],
      "js": ["src/main.tsx"]
    }
  ],
  "permissions": [
    "storage",
    "contextMenus"
  ]
}
